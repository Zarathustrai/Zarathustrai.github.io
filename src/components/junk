  function Cards() {
    //Fill array with beer data
    const beerdata = [];
    Beers.forEach((beer) => {
      beerdata.push({name: beer.name,
                  ibu: beer.ibu,
                  flavours: beer.flavours,
                  description: beer.description,
                  imagesrc: beer.imagesrc
                });
    });




      <>
      {beerdata?.map(({name, ibu, flavours, description, imagesrc}) => (
        <div key={name} className="card">
          <div className="beer">
            <img src={imagesrc} />
          </div>
          <h2>{name}</h2>
          <p>{description}</p>
        </div>
      ))}
      </>



        state = {
          loading: true,
          beers: null
        };

        async componentDidMount() {
          const url = "/data/data.json";
          const response = await fetch(url);
          const data = await response.json();
          this.setState({beers: data.Beers})
          console.log(data);
        }
